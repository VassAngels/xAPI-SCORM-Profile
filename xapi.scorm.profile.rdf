    <skos:ConceptScheme rdf:about="https://w3id.org/xapi/scorm">
        <rdf:type rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <skos:prefLabel xml:lang="en">xAPI SCORM Profile</skos:prefLabel>
        <skos:editorialNote xml:lang="en">Verbs, Activity Types and xAPI Document definitions used to represent SCORM learning experiences in xAPI.</skos:editorialNote>
        <prov:wasGeneratedBy rdf:resource="https://adlnet.gov">
        <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2016-04-03</dcterms:created>
