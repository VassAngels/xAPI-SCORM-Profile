    </rdf:Description>

    <rdf:Description rdf:about="http://adlnet.gov/expap/activities/module">
        <xapi:referencedBy rdf:resource="https://w3id.org/xapi/scorm" />
        <skos:scopeNote xml:lang="en-US">Represents a module within a SCORM course.</skos:scopeNote>
    </rdf:Description>

    <rdf:Description rdf:about="http://adlnet.gov/expap/activities/lesson">
        <xapi:referencedBy rdf:resource="https://w3id.org/xapi/scorm" />
        <skos:scopeNote xml:lang="en-US">Represents a SCORM SCO.</skos:scopeNote>
    </rdf:Description>

    <rdf:Description rdf:about="http://adlnet.gov/expap/activities/assessment">
        <xapi:referencedBy rdf:resource="https://w3id.org/xapi/scorm" />
        <skos:scopeNote xml:lang="en-US">Represents a test or assessment within a SCO.</skos:scopeNote>
    </rdf:Description>

    <rdf:Description rdf:about="http://adlnet.gov/expap/activities/interaction">
        <xapi:referencedBy rdf:resource="https://w3id.org/xapi/scorm" />
        <skos:scopeNote xml:lang="en-US">Represents an interaction within a SCO.</skos:scopeNote>
    </rdf:Description>

    <rdf:Description rdf:about="http://adlnet.gov/expap/activities/objective">
        <xapi:referencedBy rdf:resource="https://w3id.org/xapi/scorm" />
        <skos:scopeNote xml:lang="en-US">Represents a SCORM objective within a SCO.</skos:scopeNote>
    </rdf:Description>

    <rdf:Description rdf:about="http://adlnet.gov/expap/activities/attempt">
        <xapi:referencedBy rdf:resource="https://w3id.org/xapi/scorm" />
        <skos:scopeNote xml:lang="en-US">Represents an attempt on an activity.</skos:scopeNote>
    </rdf:Description>

</rdf:RDF>
